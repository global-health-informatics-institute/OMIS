
<%= turbo_stream.update "report_content" do %>
  <% if @selected_report == "HR" %>
    <%= render partial: "reports/human_resources_report", locals: {selected_report: @selected_report} %>
  <% elsif @selected_report == "Assets" %>
    <%= render partial: "reports/asset_status", locals: {selected_report: @selected_report} %>
  <% elsif @selected_report == 'Employee LOE' %>
    <%= render partial: "employee_monthly_timesheet_summary", locals: {selected_report: @selected_report} %>
  <% elsif @selected_report == 'Org LOE' %>
    <%= render partial: "show", locals: {selected_report: @selected_report} %>
  <% end %>

<% end %>

