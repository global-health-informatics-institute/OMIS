<script src="/assets/chart.umd.min.js"></script>

<% if @requisition.requisition_type == 'Petty Cash' %>
  <%= render partial: 'requested_petty_cash' %>
<% elsif @requisition.requisition_type == 'Purchase Request' %>
  <%= render partial: 'requisitions/purchase_requests/new', locals: { requisition: @requisition } %>
<% else %>
  <%= render partial: 'travel_request' %>
<% end %>